int x[10]

array = [1, 4, 2, 7]
invariant(array[x] == 4)

x -> 1

---

int x[10]
int y[10]

array = [[1, 3], [5, 7, 9]]
invariant(array[x][y] == 9)

x -> 1
y -> 2


array:

if x == 0
  if y == 0
    1
  else if y == 1
    3
  else
    0
  end
else if x == 1
  if y == 0
    5
  else if y == 1
    7
  else if y == 2
    9
  else
    0
  end
else
  0
end

invariants:

if x == 0
  0 <= y < 2
else if x == 1
  0 <= y < 3
else
  false
end
